<p id="notice"><%= notice %></p>

<p>
  <strong>Message:</strong>
  <%= @message_history.message_id %>
</p>

<p>
  <strong>Date sent:</strong>
  <%= @message_history.date_sent %>
</p>

<p>
  <strong>Student:</strong>
  <%= @message_history.student_id %>
</p>

<% @students.each do |student| %>
        <tr>
          <td><%= student.id %></td>
          <td><%= student.First_Name %></td>
          <td><%= student.Last_Name %></td>
        </tr>
      <% end %>

<%= link_to 'Edit', edit_message_history_path(@message_history) %> |
<%= link_to '<button type="button">Back</button>'.html_safe, message_histories_path %>
